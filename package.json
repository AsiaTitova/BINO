{
  "name": "startup-build-template",
  "version": "1.0.0",
  "description": "Стартовый шаблон сборки",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "^1.0.0",
    "autoprefixer": "10.2.x",
    "browser-sync": "2.26.x",
    "gulp": "4.0.x",
    "gulp-csso": "4.0.x",
    "gulp-imagemin": "7.1.x",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "9.0.x",
    "gulp-posthtml": "^3.0.5",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "4.1.x",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-svgstore": "^7.0.1",
    "gulp-webp": "^4.0.1",
    "pixel-glass": "^1.0.13",
    "postcss": "^8.2.4",
    "posthtml-include": "^1.6.0",
    "stylelint": "^13.8.0",
    "stylelint-config-htmlacademy": "0.1.x",
    "swiper": "^6.4.5"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --syntax scss",
    "test": "npm run editorconfig && npm run stylelint && eslint source/js/",
    "build": "gulp build",
    "start": "gulp start",
    "webp": "gulp webp",
    "sprite": "gulp sprite"
  },
  "browserslist": [
    "IE 11",
    "last 1 chrome version",
    "last 1 firefox version",
    "last 1 safari version"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/sass/**/*.{sass,scss}"
  ],
  "engines": {
    "node": "10"
  },
  "dependencies": {
    "del": "^6.0.0",
    "node-sass": "^5.0.0"
  }
}
